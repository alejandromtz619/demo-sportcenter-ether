{
  "identity": {
    "name": "Complejo Sport Center",
    "tone": "Elite, High-Performance, Night-Life, Luxury",
    "mission": "Transform court booking into a premium experience. No more boring spreadsheets."
  },
  "typography": {
    "primary_font": "Oswald, sans-serif",
    "secondary_font": "Manrope, sans-serif",
    "font_urls": [
      "https://fonts.googleapis.com/css2?family=Oswald:wght@300;400;500;700&family=Manrope:wght@300;400;500;600;700&display=swap"
    ],
    "weights": {
      "heading": "500",
      "body": "400",
      "bold": "700"
    },
    "rules": {
      "headings": "Uppercase, tracking-widest, tight leading. Use for 'COURT 1', 'BOOK NOW'.",
      "body": "Clean, high legibility. Use for time slots and descriptions."
    }
  },
  "colors": {
    "theme": "dark",
    "palette": {
      "background": "#050505",
      "surface": "#121212",
      "surface_highlight": "#1E1E1E",
      "primary": "#CCFF00",
      "primary_foreground": "#000000",
      "secondary": "#FFFFFF",
      "secondary_foreground": "#000000",
      "accent": "#CCFF00",
      "muted": "#27272A",
      "muted_foreground": "#A1A1AA",
      "border": "#27272A",
      "destructive": "#FF453A",
      "success": "#39FF14"
    },
    "gradients": {
      "hero_glow": "radial-gradient(circle at center, rgba(204, 255, 0, 0.15) 0%, rgba(5, 5, 5, 0) 70%)",
      "card_hover": "linear-gradient(45deg, rgba(204, 255, 0, 0.05), transparent)"
    }
  },
  "components": {
    "buttons": {
      "style": "sharp",
      "radius": "0px",
      "hover_effect": "glow_neon",
      "classes": "uppercase tracking-wider font-oswald font-bold transition-all duration-300 hover:shadow-[0_0_20px_rgba(204,255,0,0.4)] hover:bg-[#CCFF00] hover:text-black"
    },
    "cards": {
      "style": "glass_panel",
      "background": "bg-[#121212]/80 backdrop-blur-md",
      "border": "border border-white/10 hover:border-[#CCFF00]/50 transition-colors duration-300",
      "radius": "rounded-none"
    },
    "inputs": {
      "style": "minimal_underlined",
      "background": "bg-transparent",
      "border": "border-b border-white/20 focus:border-[#CCFF00]",
      "radius": "rounded-none"
    }
  },
  "layout_strategies": {
    "container_max_width": "max-w-7xl",
    "grid_system": {
      "catalog": "bento_grid",
      "dashboard": "high_density_grid"
    },
    "spacing": {
      "section_padding": "py-24",
      "element_gap": "gap-8"
    }
  },
